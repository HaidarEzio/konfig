{
  "name": "{{npmName}}",
  "version": "{{npmVersion}}",
  "description": "Client for {{appName}}",
  "author": "Konfig",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://{{gitHost}}/{{gitUserId}}/{{gitRepoId}}"
  },
  "keywords": [
    "axios",
    "typescript",
    "konfig",
    "{{npmName}}"
  ],
  "license": "Unlicense",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
{{#supportsES6}}
  "module": "./dist/esm/index.js",
  "sideEffects": false,
{{/supportsES6}}
  "scripts": {
    "build": "npm run clean && tsc{{#supportsES6}} && tsc -p tsconfig.esm.json{{/supportsES6}}",
    "clean": "rm -rf dist/",
    "prepack": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.26.1"{{#includeEventSourceParser}},{{/includeEventSourceParser}}
{{#includeEventSourceParser}}
    "eventsource-parser": "^1.0.0"{{#includeFetchAdapter}},{{/includeFetchAdapter}}
{{/includeEventSourceParser}}
{{#includeFetchAdapter}}
    "konfig-axios-fetch-adapter": "^1.0.3"
{{/includeFetchAdapter}}
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^12.11.5",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.0"
  }{{#npmRepository}},{{/npmRepository}}
{{#npmRepository}}
  "publishConfig": {
    "registry": "{{npmRepository}}"
  }
{{/npmRepository}}
}
