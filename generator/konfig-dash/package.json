{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "yarn workspaces foreach -pitv -j unlimited run build",
    "dev": "./bash-scripts/dev.sh",
    "dev-without-python": "./bash-scripts/dev.sh --no-python",
    "start-python": "./bash-scripts/start-python.sh",
    "test": "yarn build && yarn rw build api && ./node_modules/.bin/ts-node --project scripts/tsconfig.json scripts/test.ts",
    "release": "yarn workspaces foreach -tv --no-private run release-ci"
  },
  "devDependencies": {
    "@redwoodjs/core": "^3.3.1",
    "@types/wait-on": "^5.3.1",
    "wait-on": "^7.0.1"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.4",
  "name": "konfig-dash"
}
